{
  "name": "angular-webpack-workflow",
  "version": "1.0.0",
  "description": "A workflow for Angular made with Webpack",
  "scripts": {
    "build": "rimraf dist && webpack --bail --progress --profile",
    "dist": "webpack-dev-server --content-base dist/ --inline --progress --watch",
    "server": "webpack-dev-server --history-api-fallback --inline --progress --display-error-details",
    "test": "karma start",
    "test-watch": "karma start --auto-watch --no-single-run",
    "start": "npm run server"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Foxandxss/angular-webpack-workflow.git"
  },
  "homepage": "https://github.com/Foxandxss/angular-webpack-workflow",
  "dependencies": {
    "angular": "^1.5.0",
    "angular-breadcrumb": "^0.4.1",
    "angular-loading-bar": "^0.9.0",
    "angular-material": "^1.0.9",
    "angular-nvd3": "^1.0.7",
    "angular-ui-grid": "^3.1.1",
    "angular-wizard": "^0.6.1",
    "angularjs-nvd3-directives": "0.0.8",
    "babel-runtime": "^6.9.2",
    "jquery": "^3.0.0",
    "ng-annotate-webpack-plugin": "^0.1.3",
    "path": "^0.12.7",
    "weak-map": "^1.0.5"
  },
  "devDependencies": {
    "angular-animate": "^1.5.6",
    "angular-aria": "^1.5.6",
    "angular-material": "^1.0.9",
    "angular-mocks": "^1.5.0",
    "angular-ui-router": "^0.3.0",
    "autoprefixer": "^6.0.3",
    "babel-core": "^6.2.1",
    "babel-loader": "^6.2.0",
    "babel-plugin-transform-runtime": "^6.9.0",
    "babel-polyfill": "^6.9.1",
    "babel-preset-es2015": "^6.1.18",
    "copy-webpack-plugin": "^1.1.1",
    "css-loader": "^0.23.0",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.8.4",
    "html-webpack-plugin": "^2.7.1",
    "node-libs-browser": "^1.0.0",
    "null-loader": "^0.1.1",
    "postcss-loader": "^0.8.2",
    "raw-loader": "^0.5.1",
    "rimraf": "^2.5.1",
    "static-site-generator-webpack-plugin": "^2.1.0",
    "style-loader": "^0.13.0",
    "ui-router": "^1.0.0-alpha.3",
    "webpack": "^1.12.13",
    "webpack-dev-server": "^1.14.1"
  }
}
